@article{Scaillet2004,
	Abstract = { This paper introduces two new nonparametric estimators for probability density functions which have support on the non-negative real line. These kernel estimators are based on some inverse Gaussian (IG) and reciprocal inverse Gaussian (RIG) probability density functions used as kernels. We show that they share the same properties as those of gamma kernel estimators: they are free of boundary bias, always non-negative and achieve the optimal rate of convergence for the mean integrated squared error (MISE). Monte Carlo results concerning finite sample properties are reported for different distributions and sample sizes. },
	Annote = {The authors build on the method suggested by Chen but use inverse Gaussian and reciprocal inverse Gaussian kernels. They show that the estimators have optimal convergence properties, are free of boundary bias and are non-negative everywhere. 

They suggest that the log-normal kernel is not suitable for kernel density estimation which may be true in the formulation where the variable $x$ is considered a parameter of the kernel rather than the univariate random variable. Because of their formulation following Chen, the estimator is not properly normalised.

They derive a plugin estimator using the log-normal distribution as a reference distribution. They encounter the same problem that the Gamma distribution can only be used as a reference distribution if its parameters satisfy certain inequalities. Their plugin estimator apparently doesn't work very well.},
	Author = {Scaillet, O.},
	Cites = {Chen2000},
	Date-Added = {2015-12-16 18:48:16 +0000},
	Date-Modified = {2015-12-16 18:48:16 +0000},
	Doi = {10.1080/10485250310001624819},
	Journal = {Journal of Nonparametric Statistics},
	Keywords = {Density estimation; inverse Gaussian},
	Number = {1-2},
	Pages = {217--226},
	Title = {Density estimation using inverse and reciprocal inverse Gaussian kernels},
	Volume = {16},
	Year = {2004},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1080/10485250310001624819}}

@article{Jin2003,
	Abstract = {In this article we extend the class of non-negative, asymmetric kernel density estimators and propose Birnbaum-Saunders (BS) and lognormal (LN) kernel density functions. The density functions have bounded support on [0,1). Both BS and LN kernel estimators are free of boundary bias, non-negative, with natural varying shape, and achieve the optimal rate of convergence for the mean integrated squared error. We apply BS and LN kernel density estimators to high frequency intraday time duration data. The comparisons are made on several nonparametric kernel density estimators. BS and LN kernels perform better near the boundary in terms of bias reduction.},
	Annote = {The authors build on the method suggested by Chen but use Birnbaum-Saunders and log-normal kernels. They show that the estimators have optimal convergence properties, are free of boundary bias and are non-negative everywhere. Because of their formulation the density is not necessarily properly normalised.

They almost provide plugin estimators but do not follow through by using a reference distribution.},
	Author = {Jin, Xiaodong and Kawczak, Janusz},
